{
  "handler": {
    "type": "Router",
    "audit": "global",
    "baseURI": "https://forgerock.org:443",
    "capture": "all"
  },
  "session": "JwtSession",
  "heap": [
    {
      "name": "LogSink",
      "type": "ConsoleLogSink",
      "config": {
        "level": "DEBUG"
      }
    },
    {
      "name": "IGKeyStore",
      "type": "KeyStore",
      "config": {
        "url": "file:///var/secrets/openig/keystore.jks",
        "password": "changeit"
      }
    },
    {
      "name": "JwtSession",
      "type": "JwtSession",
      "config": {
        "keystore": "IGKeyStore",
        "alias": "jwe-key",
        "password": "changeit",
        "cookieName": "OpenIG",
        "sharedSecret": "OpenIG is Fun"
        }
    },
    {
      "name": "capture",
      "type": "CaptureDecorator",
      "config": {
        "captureEntity": true,
        "_captureContext": true
      }
    }
  ]
}